{
  "name": "soshi",
  "version": "1.0.4",
  "description": "an xperimental alternatives to webcomponents",
  "main": "build/bundle.js",
  "scripts": {
    "test": "xo",
    "build": "webpack --config webpack.build.config.js",
    "start": "webpack-dev-server --content-base build --hot --inline",
    "surge": "webpack --config webpack.config.js && surge ./build --domain soshi.surge.sh",
    "publish": "npm publish"
  },
  "xo": {
    "space": true,
    "rules": {
      "import": {
        "noUnresolved": false
      },
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ]
    },
    "globals": [
      "document",
      "window"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stagfoo/soshi.git"
  },
  "author": "Alexander Ravenking",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stagfoo/soshi/issues"
  },
  "homepage": "https://github.com/stagfoo/soshi#readme",
  "devDependencies": {
    "@types/node": "^8.5.9",
    "awesome-typescript-loader": "^3.4.1",
    "fairybread": "^2.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "standard-version": "^4.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "vue": "^2.5.13",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.1.0",
    "webpack-dev-server": "^2.11.1",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "bel": "^5.1.5",
    "hyperx": "^2.3.2",
    "mdn-polyfills": "^5.5.0",
    "vel": "^1.2.0",
    "virtual-dom": "^2.1.1",
    "yo-yo": "^1.4.1"
  }
}
